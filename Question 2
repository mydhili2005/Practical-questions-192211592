#include <stdio.h>

int main() {
    int m, n, k, i;

    printf("Enter the value of m: ");
    scanf("%d", &m);

    printf("Enter the value of n: ");
    scanf("%d", &n);

    printf("Enter the value of k: ");
    scanf("%d", &k);

    for (i = m; i <= n; i += k+1) {
        printf("%d ", i);
    }

return 0;
}
